  "scripts": {
    "build:development": "parallel-webpack --config ./config/webpack.development.config.js",
    "build:production": "parallel-webpack --config ./config/webpack.production.config.js",
    "build:development:watch": "parallel-webpack --config ./config/webpack.development.config.js --watch",
    "build:production:watch": "parallel-webpack --config ./config/webpack.production.config.js --watch",
    "lint": "npm run lint:tslint && npm run lint:eslint",
    "lint:eslint": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern '{scripts,src}/**/*.{js,jsx}'",
    "lint:tslint": "tslint -c tslint.json -e '**/*.example.*' --project tsconfig.json '{scripts,src}/**/*.{ts,tsx}'",
    "postinstall": "yarn run postinstall:git-lfs",
    "postinstall:git-lfs": "git lfs update --force",
    "ci": "yarn install --frozen-lockfile",
    "server": "ts-node --project tsconfig.json --require tsconfig-paths/register --files server.ts",
    "server:watch": "nodemon -e ts --watch 'build/**/*' --watch 'server/**/*' --watch 'server.ts' --exec yarn server",
    "server:watch-fast": "tsnd --project tsconfig.json --require tsconfig-paths/register --poll --respawn --transpile-only --no-deps --exit-child --watch build, server -- server.ts"
  },
